@model IEnumerable<Cap.Domain.Models.Gen.Agenda>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Contato)
        </th>
        <th>
            Telefones
        </th>
        <th>
            Email
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Contato)
        </td>
        <td>
            @{ Html.RenderAction("Telefones", "Telefone", new { idAgenda = item.Id }); }
        </td>
        <td>
            @{ Html.RenderAction("Emails", "Email", new { idAgenda = item.Id }); }
        </td>
        <td>
            | @Html.ActionLink("Detalhes", "Details", new { id=item.Id, nome = (string)ViewBag.Nome }) |
        </td>
    </tr>
}

</table>